#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules/.pnpm/eslint@8.57.0/node_modules/eslint/bin/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules/.pnpm/eslint@8.57.0/node_modules/eslint/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules/.pnpm/eslint@8.57.0/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules/.pnpm/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/node_modules:/mnt/c/Users/ibizsoftware/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules/.pnpm/@eslint-community+eslint-utils@4.4.0_eslint@8.57.0/node_modules/@eslint-community/eslint-utils/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules/.pnpm/@eslint-community+eslint-utils@4.4.0_eslint@8.57.0/node_modules/@eslint-community/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules/.pnpm/@eslint-community+eslint-utils@4.4.0_eslint@8.57.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules/.pnpm/eslint@8.57.0/node_modules/eslint/bin/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules/.pnpm/eslint@8.57.0/node_modules/eslint/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules/.pnpm/eslint@8.57.0/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules/.pnpm/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/node_modules:/mnt/c/Users/ibizsoftware/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules/.pnpm/@eslint-community+eslint-utils@4.4.0_eslint@8.57.0/node_modules/@eslint-community/eslint-utils/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules/.pnpm/@eslint-community+eslint-utils@4.4.0_eslint@8.57.0/node_modules/@eslint-community/node_modules:/mnt/c/Users/ibizsoftware/hush-auth/backend/node_modules/.pnpm/@eslint-community+eslint-utils@4.4.0_eslint@8.57.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../eslint@8.57.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../../eslint@8.57.0/node_modules/eslint/bin/eslint.js" "$@"
fi
